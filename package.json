{
  "name": "vue-skeleton",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc && vite build",
    "preview": "vite preview",
    "test": "vitest run",
    "test-ui": "vitest --ui",
    "coverage": "vitest run --coverage",
    "lint": "eslint --ext .ts,.js,.vue, .",
    "stylelint": "stylelint \"**/*.{css,scss,sass,vue}\"",
    "lint-fix": "yarn lint --fix && yarn stylelint --fix",
    "prepare": "husky install"
  },
  "lint-staged": {
    "*.{js,ts,vue}": [
      "eslint --fix"
    ],
    "*.{css,scss,sass,vue}": [
      "stylelint --fix"
    ]
  },
  "dependencies": {
    "js-cookie": "^3.0.5",
    "qs": "^6.11.2",
    "vue": "^3.3.4",
    "vue-router": "4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@rushstack/eslint-patch": "^1.3.3",
    "@types/js-cookie": "^3.0.3",
    "@types/node": "^20.6.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vitest/coverage-v8": "^0.34.4",
    "@vitest/ui": "^0.34.4",
    "@vue/eslint-config-typescript": "^11.0.3",
    "@vue/test-utils": "^2.4.1",
    "eslint": "^8.49.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^8.0.0",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "postcss-html": "^1.5.0",
    "prettier": "^3.0.3",
    "sass": "^1.66.1",
    "stylelint": "^15.10.3",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^11.0.0",
    "typescript": "5.1.6",
    "vite": "^4.4.5",
    "vitest": "^0.34.4",
    "vue-tsc": "^1.8.5"
  }
}
